{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
<table class="table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Content</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
{% for data in datas %}
    <tr>
        <th scope ="row">{{data.title}}</th>
        <td>{{data.content}}</td>
</table>
{% endfor %}
{% endblock %}
